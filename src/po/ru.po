#, c-format
msgid ""
"\n"
"Options:\n"
msgstr ""
"\n"
"Параметры:\n"

#: src/parse_args.c:542
msgid "use helper program for password prompting\n"
msgstr "использовать вспомогательную программу для ввода пароля\n"

